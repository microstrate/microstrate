      "id": "post_workflows__workflowId__actions_trigger",
      "operationId": "TriggerWorkflow",
      "name": "Trigger Workflow",
      "description": "Trigger a new instance of a Maestro workflow",
      "schema": {
        "tags": [
          "Workflows"
        ],
        "input": {
          "$schema": "$ref:schema",
          "$id": "https://datahub.com/integration/Maestro-API/post_workflows__workflowId__actions_trigger/input.schema.json",
          "type": "object",
          "properties": {
            "url": {
              "type": "string",
              "const": "/workflows/{workflowId}/actions/trigger"
            },
            "method": {
              "type": "string",
